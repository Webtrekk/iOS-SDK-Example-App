<?xml version="1.0" encoding="utf-8"?>
<webtrekkConfiguration>
    <!--the version number for this configuration file -->
    <version>2</version>
    <!--the webtrekk trackDomain where the requests are send -->
    <trackDomain>https://q3.webtrekk.net</trackDomain>

    <!--customers trackid-->
    <trackId>123451234512345</trackId>
    
    <!-- measure only a subset of the users -->
    <sampling>0</sampling>
    <!-- interval between the requests are send in seconds -->
    <sendDelay>1</sendDelay>
    <!--maximum amoount of requests to store when the user is offline -->
    <maxRequests>5000</maxRequests>
    
    <!--optional settings -->
    <!--automaticly track activities onStart method -->
    <autoTracked>false</autoTracked>
    
    
    <!--track if there was an application update -->
    <autoTrackAppUpdate>true</autoTrackAppUpdate>
    <!--track the advertiser id -->
    <autoTrackAdvertiserId>true</autoTrackAdvertiserId>
    <!--track the app versions name -->
    <autoTrackAppVersionName>true</autoTrackAppVersionName>
    <!--track the app versions code -->
    <autoTrackAppVersionCode>true</autoTrackAppVersionCode>
    <!--track the devices screen orientation -->
    <autoTrackScreenOrientation>true</autoTrackScreenOrientation>
    <!--track the current connection type -->
    <autoTrackConnectionType>true</autoTrackConnectionType>
    <!--track if the user has opted out for advertisement on google plays -->
    <autoTrackAdvertisementOptOut>true</autoTrackAdvertisementOptOut>
    <!--sends the size of the current locally stored urls in a custom parameter -->
    <autoTrackRequestUrlStoreSize>true</autoTrackRequestUrlStoreSize>
    
    <!--enables the remote xml configuration -->
    <enableRemoteConfiguration>false</enableRemoteConfiguration>
    <trackingConfigurationUrl>https://d1r27qvpjiaqj3.cloudfront.net/1111111111111111/36704.xml</trackingConfigurationUrl>
    <!--resend onStart time, this is the timeout for auto tracked sessions in case an screen was paused -->
    <resendOnStartEventTime>30</resendOnStartEventTime>
    
    <!--enable or disable exception logging. By default is disabled -->
    <errorLogEnable>true</errorLogEnable>
    <!--set log level to do loging 1- only fatals 2 - fatal and catched, 3- all logs. By default = 3 -->
    <errorLogLevel>3</errorLogLevel>
    
    <!--global tracking parameter -->
    <globalTrackingParameter>
        <!--define global tracking parameter which are send with every request, the key has to match a valid parameter name!
         entries made here are available as default parameters in the trackingparameter instance -->
        <parameter id="ADVERTISEMENT" key="GLADVERTISEMENTKey"></parameter>
        <parameter id="ADVERTISEMENT_ACTION" key="GLADVERTISEMENT_ACTIONKey"></parameter>
        <parameter id="BIRTHDAY" key="GLBIRTHDAYKey"></parameter>
        <parameter id="CITY" key="GLCITYKey"></parameter>
        <parameter id="COUNTRY" key="GLCOUNTRYKey"></parameter>
        <parameter id="CURRENCY" key="GLCURRENCYKey"></parameter>
        <parameter id="CUSTOMER_ID" key="GLUSERIDKey"></parameter>
        <parameter id="EMAIL" key="GLEMAILKey"></parameter>
        <parameter id="EMAIL_RID" key="GLEMAIL_RIDKey"></parameter>
        <parameter id="GNAME" key="GLNAMEKey"></parameter>
        <parameter id="GENDER" key="GLGENDERKey"></parameter>
        <parameter id="INTERN_SEARCH" key="GLINTERN_SEARCHKey"></parameter>
        <parameter id="IP_ADDRESS" key="GLIP_ADDRESSKey"></parameter>
        <parameter id="SNAME" key="GLSNAMEKey"></parameter>
        <parameter id="NEWSLETTER" key="GLNEWSLETTERKey"></parameter>
        <parameter id="PAGE_URL" key="GLPAGE_URLKey"></parameter>
        <parameter id="PHONE" key="GLPHONEKey"></parameter>
        <parameter id="STREET" key="GLSTREETKey"></parameter>
        <parameter id="STREETNUMBER" key="GLSTREETNUMBERKey"></parameter>
        <parameter id="ZIP" key="GLZIPKey"></parameter>
        <parameter id="PRODUCT" key="GLPRODUCTKey"></parameter>
        <parameter id="PRODUCT_COST" key="GLPRODUCT_COSTKey"></parameter>
        <parameter id="PRODUCT_COUNT" key="GLPRODUCT_COUNTKey"></parameter>
        <parameter id="PRODUCT_STATUS" key="GLPRODUCT_STATUSKey"></parameter>
        <parameter id="ORDER_NUMBER" key="GLORDER_NUMBERKey"></parameter>
        <parameter id="ORDER_TOTAL" key="GLORDER_TOTALKey"></parameter>
        <parameter id="VOUCHER_VALUE" key="GLVOUCHER_VALUEKey"></parameter>


        <!--define the global page parameter, the key is the index,  -->
        <pageParameter>
            <parameter id="1" key="KeyGlobalConfPage"></parameter>
            <parameter id="30" key="KeyPageGlobalConfOver"></parameter>
            <parameter id="3">PageGlobalConfStaticValue</parameter>
            <parameter id="784" key="KeyRequestSizeConf"/>
        </pageParameter>
        <pageCategories>
            <parameter id="1" key="KeyPageCatGlobalConf"></parameter>
            <parameter id="10" key="KeyPageCatGlobalConfOver"></parameter>
            <parameter id="3">PageCatGlobalConfStaticValue</parameter>
        </pageCategories>
        <sessionParameter>
            <parameter id="1" key="KeyGlobalConfSession"></parameter>
            <parameter id="10" key="KeySessionGlobalConfOver"></parameter>
            <parameter id="3">SessionGlobalConfStaticValue</parameter>
            <parameter id="804" key="KeyVersionOverGlobalConf"/>
            <parameter id="809" key="KeyAdvertizingIDGlobalConf"/>
        </sessionParameter>
        <ecomParameter>
            <parameter id="1" key="KeyGlobalConfEcom"></parameter>
            <parameter id="10" key="KeyEcomGlobalConfOver"></parameter>
            <parameter id="3">EconGlobalConfStaticValue</parameter>
        </ecomParameter>
        <userCategories>
            <parameter id="1" key="KeyUserGlobalConf"></parameter>
            <parameter id="10" key="KeyUserGlobalConfOver"></parameter>
            <parameter id="3">UserGlobalConfStaticValue</parameter>
        </userCategories>
        <adParameter>
            <parameter id="1" key="KeyAdvGlobalConf"></parameter>
            <parameter id="10" key="KeyAdvGlobalConfOver"></parameter>
            <parameter id="3">AdvGlobalConfStaticValue</parameter>
        </adParameter>
        <productCategories>
            <parameter id="1" key="KeyPrCatGlobalConf"></parameter>
            <parameter id="11" key="KeyPrCatGlobalConfOver"></parameter>
        </productCategories>
        <actionParameter>
            <parameter id="1">test_actionparam1</parameter>
            <parameter id="2" key="Key2"></parameter>
        </actionParameter>
        <mediaCategories>
            <parameter id="1">test_mediacategory1</parameter>
            <parameter id="2" key="Key2"></parameter>
        </mediaCategories>
        
    </globalTrackingParameter>
    
    <!--screen configurations -->
    <screen>
        <classname>ViewController</classname>
        <mappingname>autoPageName</mappingname>
        <autoTracked>true</autoTracked>
        <screenTrackingParameter>
            <parameter id="ADVERTISEMENT" key="SCADVERTISEMENTKey"></parameter>
            <parameter id="ADVERTISEMENT_ACTION" key="SCADVERTISEMENT_ACTIONKey"></parameter>
            <parameter id="BIRTHDAY" key="SCBIRTHDAYKey"></parameter>
            <parameter id="CITY" key="SCCITYKey"></parameter>
            <parameter id="COUNTRY" key="SCCOUNTRYKey"></parameter>
            <parameter id="CURRENCY" key="SCCURRENCYKey"></parameter>
            <parameter id="CUSTOMER_ID" key="SCUSERIDKey"></parameter>
            <parameter id="EMAIL" key="SCEMAILKey"></parameter>
            <parameter id="EMAIL_RID" key="SCEMAIL_RIDKey"></parameter>
            <parameter id="GNAME" key="SCNAMEKey"></parameter>
            <parameter id="GENDER" key="SCGENDERKey"></parameter>
            <parameter id="INTERN_SEARCH" key="SCINTERN_SEARCHKey"></parameter>
            <parameter id="IP_ADDRESS" key="SCIP_ADDRESSKey"></parameter>
            <parameter id="SNAME" key="SCSNAMEKey"></parameter>
            <parameter id="NEWSLETTER" key="SCNEWSLETTERKey"></parameter>
            <parameter id="PAGE_URL" key="SCPAGE_URLKey"></parameter>
            <parameter id="PHONE" key="SCPHONEKey"></parameter>
            <parameter id="STREET" key="SCSTREETKey"></parameter>
            <parameter id="STREETNUMBER" key="SCSTREETNUMBERKey"></parameter>
            <parameter id="ZIP" key="SCZIPKey"></parameter>
            <parameter id="PRODUCT" key="SCPRODUCTKey"></parameter>
            <parameter id="PRODUCT_COST" key="SCPRODUCT_COSTKey"></parameter>
            <parameter id="PRODUCT_COUNT" key="SCPRODUCT_COUNTKey"></parameter>
            <parameter id="PRODUCT_STATUS" key="SCPRODUCT_STATUSKey"></parameter>
            <parameter id="ORDER_NUMBER" key="SCORDER_NUMBERKey"></parameter>
            <parameter id="ORDER_TOTAL" key="SCORDER_TOTALKey"></parameter>
            <parameter id="VOUCHER_VALUE" key="SCVOUCHER_VALUEKey"></parameter>
            
            
            <!--define the screen page parameter, the key is the index,  -->
            <pageParameter>
                <parameter id="1" key="KeyScreenConfPage"></parameter>
                <parameter id="30" key="KeyPageScreenConfOver"></parameter>
                <parameter id="3">PageScreenConfStaticValue</parameter>
            </pageParameter>
            <pageCategories>
                <parameter id="1" key="KeyPageCatScreenConf"></parameter>
                <parameter id="10" key="KeyPageCatScreenConfOver"></parameter>
                <parameter id="3">PageCatScreenConfStaticValue</parameter>
            </pageCategories>
            <sessionParameter>
                <parameter id="1" key="KeyScreenConfSession"></parameter>
                <parameter id="10" key="KeySessionScreenConfOver"></parameter>
                <parameter id="3">SessionScreenConfStaticValue</parameter>
            </sessionParameter>
            <ecomParameter>
                <parameter id="1" key="KeyScreenConfEcom"></parameter>
                <parameter id="10" key="KeyEcomScreenConfOver"></parameter>
                <parameter id="3">EconScreenConfStaticValue</parameter>
            </ecomParameter>
            <userCategories>
                <parameter id="1" key="KeyUserScreenConf"></parameter>
                <parameter id="10" key="KeyUserScreenConfOver"></parameter>
                <parameter id="3">UserScreenConfStaticValue</parameter>
            </userCategories>
            <adParameter>
                <parameter id="1" key="KeyAdvScreenConf"></parameter>
                <parameter id="10" key="KeyAdvScreenConfOver"></parameter>
                <parameter id="3">AdvScreenConfStaticValue</parameter>
            </adParameter>
            <productCategories>
                <parameter id="1" key="KeyPrCatScreenConf"></parameter>
                <parameter id="11" key="KeyPrCatScreenConfOver"></parameter>
                <parameter id="3">PrCatScreenConfStaticValue</parameter>
            </productCategories>
            <actionParameter>
                <parameter id="1">test_actionparam1</parameter>
                <parameter id="2" key="Key2"></parameter>
            </actionParameter>
            <mediaCategories>
                <parameter id="1">test_mediacategory1</parameter>
                <parameter id="2" key="Key2"></parameter>
            </mediaCategories>
        </screenTrackingParameter>
    </screen>
</webtrekkConfiguration>
